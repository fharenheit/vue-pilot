 'download', 'display2', 'display1', 'diskette', 'date', 'cup', 'culture', 'crop', 'credit', 'copy-file', 'config', 'compass', 'comment', 'coffee', 'cloud', 'clock', 'check', 'chat', 'cart', 'camera', 'call', 'calculator', 'browser', 'box2', 'box1', 'bookmarks', 'bicycle', 'bell', 'battery', 'ball', 'back', 'attention', 'anchor', 'albums', 'alarm', 'airplay']\n\nexport default {\n  name: 'icons-stroke-icons-7',\n  metaInfo: {\n    title: 'Stroke Icons 7 - Icons'\n  },\n  data: () => ({\n    icons: iconList.slice(0)\n  }),\n  methods: {\n    getIconClass (icon) {\n      return `.pe-7s-${icon}`\n    },\n    search ($event) {\n      const val = String($event.target.value).replace(/^\\s+|\\s+$/g, '')\n\n      if (!val) {\n        this.icons = iconList.slice(0)\n        return\n      }\n\n      this.icons = iconList.reduce((res, icon) => {\n        if (icon.indexOf(val) !== -1) res.push(icon)\n        return res\n      }, [])\n    }\n  }\n}\n</script>\n"]}]}