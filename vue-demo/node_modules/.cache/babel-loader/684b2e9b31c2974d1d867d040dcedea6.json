         <div class=\"contact-content-about\">\n              <h5 class=\"contact-content-name mb-1\"><a href=\"javascript:void(0)\" class=\"text-dark\">{{contact.name}}</a></h5>\n              <div class=\"contact-content-user text-muted small mb-2\">@{{contact.username}}</div>\n              <div class=\"small\">\n                <div v-if=\"contact.company\">{{contact.company}}</div>\n                <div v-if=\"contact.phone\">{{contact.phone}}</div>\n              </div>\n              <hr class=\"border-light\">\n              <div>\n                <a href=\"javascript:void(0)\" class=\"text-secondary\"><span class=\"ion ion-md-mail\"></span></a> &nbsp;&nbsp;\n                <a href=\"javascript:void(0)\" class=\"text-secondary\"><span class=\"ion ion-ios-chatbubbles\"></span></a> &nbsp;&nbsp;\n                <span class=\"text-lighter\">|</span> &nbsp;&nbsp;\n                <a href=\"javascript:void(0)\" class=\"text-twitter\"><span class=\"ion ion-logo-twitter\"></span></a> &nbsp;&nbsp;\n                <a href=\"javascript:void(0)\" class=\"text-facebook\"><span class=\"ion ion-logo-facebook\"></span></a> &nbsp;&nbsp;\n                <a href=\"javascript:void(0)\" class=\"text-instagram\"><span class=\"ion ion-logo-instagram\"></span></a>\n              </div>\n            </div>\n          </div>\n        </b-card>\n\n      </div>\n    </div><!-- / .row -->\n\n  </div>\n</template>\n\n<!-- Page -->\n<style src=\"@/vendor/styles/pages/contacts.scss\" lang=\"scss\"></style>\n\n<script>\nexport default {\n  name: 'pages-contacts',\n  metaInfo: {\n    title: 'Contacts - Pages'\n  },\n  data: () => ({\n    viewMode: 'col',\n\n    contactsData: [\n      {\n        avatar: '9.png',\n        name: 'Amanda Warner',\n        username: 'awarner',\n        company: 'Company Inc.',\n        phone: '+0 (123) 456 7891'\n      }, {\n        avatar: '5.png',\n        name: 'Nelle Maxwell',\n        username: 'nmaxwell',\n        company: 'Company Inc.',\n        phone